{
  "name": "@backtrade/api",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --legacy-watch --watch src --ext ts,js,json --exec tsx src/server.ts",
    "build": "tsc --project tsconfig.json && node esbuild.config.cjs",
    "build:tsc": "tsc --project tsconfig.json",
    "build:bundle": "node esbuild.config.cjs",
    "start": "node dist/server.bundle.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@backtrade/types": "workspace:*",
    "dotenv": "^17.2.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "5.1.0",
    "express-rate-limit": "8.1.0",
    "helmet": "8.1.0",
    "pino": "10.1.0",
    "pino-http": "11.0.0",
    "prom-client": "^15.1.3",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@backtrade/tsconfig": "workspace:*",
    "@eslint/js": "^9.38.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/node": "24.8.1",
    "esbuild": "0.25.11",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "jest": "30.2.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.46.2"
  },
  "license": "PROPRIETARY",
  "author": "REICHHART Damien"
}
