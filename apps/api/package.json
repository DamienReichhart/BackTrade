{
  "name": "@backtrade/api",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --legacy-watch --watch src --ext ts,js,json --exec tsx src/server.ts",
    "build": "esbuild src/server.ts --bundle --platform=node --outfile=dist/server.js --format=cjs --target=node24",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:init": "pnpm prisma:generate && pnpm prisma:deploy && pnpm prisma:seed",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:seed": "prisma db seed"
  },
  "dependencies": {
    "@backtrade/types": "workspace:*",
    "@backtrade/utils": "workspace:*",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "argon2": "^0.44.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "5.1.0",
    "express-rate-limit": "8.2.1",
    "helmet": "8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "pino": "10.1.0",
    "pino-http": "11.0.0",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@backtrade/eslint-config": "workspace:*",
    "@backtrade/tsconfig": "workspace:*",
    "@eslint/js": "^9.39.1",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "5.0.5",
    "@types/jest": "30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "24.10.1",
    "@types/pg": "^8.15.6",
    "esbuild": "0.27.0",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "jest": "30.2.0",
    "nodemon": "^3.1.11",
    "pino-pretty": "^13.1.2",
    "prisma": "^7.0.1",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "8.47.0"
  },
  "license": "PROPRIETARY",
  "author": "REICHHART Damien"
}
