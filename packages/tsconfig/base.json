{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext", "DOM"],
    "module": "esnext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true
  }
}
